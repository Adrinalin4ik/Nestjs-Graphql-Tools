{
  "name": "nestjs-graphql-tools",
  "version": "0.10.1",
  "description": "Nestjs Graphql automation library for building performant API",
  "author": "Alexey Panfilkin",
  "private": false,
  "license": "GPL-3.0-only",
  "main": "dist-lib/index.js",
  "homepage": "https://adrinalin4ik.github.io/Nestjs-Graphql-Tools",
  "repository": "https://github.com/Adrinalin4ik/Nestjs-Graphql-Tools",
  "keywords": [
    "graphql",
    "polymorphic",
    "resolver",
    "typeorm",
    "nestjs",
    "dataloader",
    "query",
    "auto",
    "n+1",
    "graphql-tools",
    "@nestjs",
    "hasura",
    "typescript",
    "nestjs"
  ],
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=16.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run cleanup && tsc -p lib.tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "cleanup": "rm lib.tsconfig.tsbuildinfo || true && rm -rf dist-lib || true",
    "release": "npm run build && release-it",
    "db:migrate": "typeorm migration:run"
  },
  "dependencies": {
    "dataloader": "^2.2.2"
  },
  "peerDependencies": {
    "@nestjs/graphql": ">8.0.0",
    "typeorm": ">0.3.0"
  },
  "devDependencies": {
    "@apollo/gateway": "^2.9.3",
    "@apollo/server": "^4.11.2",
    "@nestjs/apollo": "^12.2.1",
    "@nestjs/cli": "^10.4.7",
    "@nestjs/common": "^10.4.6",
    "@nestjs/core": "^10.4.6",
    "@nestjs/graphql": ">=12.2.1",
    "@nestjs/platform-express": "^10.4.6",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.6",
    "@nestjs/typeorm": "^10.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "^22.9.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "config": "^3.3.12",
    "eslint": "^9.14.0",
    "graphql": "^16.9.0",
    "graphql-type-json": "^0.3.2",
    "jest": "29.7.0",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "release-it": "^17.10.0",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "^9.5.1",
    "ts-morph": "^24.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typeorm": ">=0.3.20",
    "typescript": "^5.6.3"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "release-it": {
    "requireBranch": "master"
  }
}
