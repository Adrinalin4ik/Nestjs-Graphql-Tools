{
  "name": "nestjs-graphql-tools",
  "version": "0.8.2",
  "description": "Nestjs Graphql automation library for building performant API",
  "author": "Alexey Panfilkin",
  "private": false,
  "license": "GNU GPLv3",
  "main": "dist-lib/index.js",
  "homepage": "https://adrinalin4ik.github.io/Nestjs-Graphql-Tools",
  "repository": "https://github.com/Adrinalin4ik/Nestjs-Graphql-Tools",
  "keywords": [
    "graphql",
    "polymorphic",
    "resolver",
    "typeorm",
    "nestjs",
    "dataloader",
    "query",
    "auto",
    "n+1",
    "graphql-tools",
    "@nestjs",
    "hasura",
    "typescript",
    "nestjs"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run cleanup && tsc -p lib.tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "cleanup": "rm lib.tsconfig.tsbuildinfo || true && rm -rf dist-lib || true",
    "release": "npm run build && release-it",
    "db:migrate": "typeorm migration:run"
  },
  "dependencies": {
    "@nestjs/graphql": ">8.0.0",
    "dataloader": "^2.1.0",
    "typeorm": ">0.3.0"
  },
  "devDependencies": {
    "@apollo/gateway": "^2.5.5",
    "@apollo/server": "^4.9.4",
    "@nestjs/apollo": "^12.0.9",
    "@nestjs/cli": "^10.1.18",
    "@nestjs/common": "^10.2.7",
    "@nestjs/core": "^10.2.7",
    "@nestjs/graphql": "^12.0.9",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@nestjs/typeorm": "^10.0.0",
    "@types/express": "^4.17.18",
    "@types/jest": "29.5.5",
    "@types/node": "^20.8.2",
    "@types/supertest": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "apollo-server-express": "^3.12.1",
    "axios": "^1.5.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "eslint": "^8.50.0",
    "graphql": "^16.8.1",
    "graphql-type-json": "^0.3.2",
    "jest": "29.7.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "release-it": "^16.2.1",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-morph": "^20.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "release-it": {
    "requireBranch": "master"
  }
}
